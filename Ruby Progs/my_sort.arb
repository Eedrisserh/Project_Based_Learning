def my_sort(param_1, param_2)
    if param_2 == "asc"
          return param_1 = param_1.sort
      elsif param_2 == "desc"
          return param_1 = param_1.sort.reverse
      else
        return param_1
      end
    end


puts(my_sort([1, 2, 1], "asc"))
puts(my_sort([2, 1, -1], "asc"))
puts(my_sort([2, 1, -1], "desc"))
puts(my_sort([1, 7, 9, 2, -1], "blahblah"))
